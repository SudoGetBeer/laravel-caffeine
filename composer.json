{
    "name": "genealabs/laravel-caffeine",
    "description": "Keeping Your Laravel Forms Awake",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",        
        "SudoGetBeer/model": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^6.0",
        "orchestra/testbench-dusk": "^6.0",
        "orchestra/testbench": "^6.0",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^8.5",
        "php-coveralls/php-coveralls" : "^2.2",
        "sebastian/phpcpd": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",            
            "url": "https://github.com/aidan-casey/model"
        }
    ],
    "config": {
        "preferred-install": "source"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelCaffeine\\Providers\\Service"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
